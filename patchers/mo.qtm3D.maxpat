					"patching_rect" : [ 20.0, 308.5, 993.0, 385.0 ],
					"text" : "# Determine whether the bodies exist in the bundle\n/Npnts = length(/points),\n/addr_in = \"/3d\"+/points,                                             # the list of input addresses we expect in the bundle\n/addr_out = \"/modosc/points\"+/points+\"/pos\",                          # the list of input addresses we expect in the bundle\n/process = map(lambda([in], /tmp=value(in), bound(/tmp)), /addr_in), \t# test whether each address has data bound to it\n/counter = aseq(0, /Npnts-1),                                         # a list of indeces [0,..,N-1]    \n\n\n# Define a function which reformats the data (i.e. performs the action of this descriptor)\n/fnProcess = \"lambda([index],        # input: the index of the point in the list of points\n  /addr_src = /addr_in[[index]],     # the address where we expect to find the input data\n  /addr_res = /addr_out[[index]],    # the address where we will put the result\n  /indata = value(/addr_src),        # the actual input data (position in this case)\n  \n  # Assign the output to the output address\n  assign( value(/addr_res), /indata),\n\n  # Delete the original qtm data\n  map(delete, /addr_src)\n)\",\n\n# Apply the processing function using the list of indeces and whether the data are bound, as indicated in /process \nmap(lambda([body_idx,process], if(process, apply(readstring(/fnProcess), body_idx))), /counter, /process),\n\n# Delete all the addresses we defined\ndelete(/Npnts), delete(/addr_in), delete(/addr_out), delete(/process), delete(/counter),\ndelete(/addr_src), delete(/addr_res), delete(/indata), delete(/points), delete(/fnProcess),delete(/tmp) ",
