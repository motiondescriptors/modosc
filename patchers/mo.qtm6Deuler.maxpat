					"patching_rect" : [ 17.0, 363.5, 892.0, 372.0 ],
					"text" : "# Determine whether the bodies exist in the bundle\n/addr = \"/6d_euler\"+/bodies,                                       # the list of addresses we expect in the bundle\n/process = map(lambda([in], /tmp=value(in), bound(/tmp)), /addr), \t\t # test whether each address has data bound to it   \ndelete(/tmp), delete(/addr),                                       # clean up\n\n# Define a function which reformats the data (i.e. performs the action of this descriptor)\n/ps = \"/pos\",     # but first define some strings that will be useful, since we can't use \" inside lambda\n/rt = \"/rot_rpy\",\n/pre = \"/6d_euler\",\n/pnts = \"/modosc/points\",\n\n/fnProcess = \"lambda([body],\n  /addr_src = /pre+body,                     # the address where we expect to find the data\n  /tmp_data = value(/addr_src),              # the actual data\n  assign( /pnts+body+/ps, /tmp_data[[0,1,2]]),     # reformat and assign to /points/body/pos_xys, and...\n  assign( /pnts+body+/rt, /tmp_data[[3,4,5]]),     # ...to /points/body/rot_rpy\n  map(delete,/addr_src)                      # delete the orginal data from qtm\n)\",\n\n# Apply the processing function using the list of addresses and whether they are bound, as indicated in /process \nmap(lambda([body,process], if(process, apply(readstring(/fnProcess), body))), /bodies, /process),\n\n# Delete all the addresses we defined\ndelete(/tmp_data), delete(/addr_src), delete(/pre), delete(/rt), delete(/ps), delete(/pnts),\ndelete(/fnProcess), delete(/process), delete(/bodies)\n",
